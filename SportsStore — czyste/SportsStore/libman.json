{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "bootstrap@4.0.0",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "unpkg",
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/lib/fontawesome/"
    },
    {
      "provider": "unpkg",
      "library": "jquery@3.2.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "jquery-validation@1.17.0",
      "destination": "wwwroot/lib/jqueryvalidation/"
    },
    {
      "provider": "unpkg",
      "library": "jquery-validation-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jqueryvalidationunobtusive/"
    }
  ]
}